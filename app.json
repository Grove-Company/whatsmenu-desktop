{
  "expo": {
    "name": "Whatsmenu Impressora 2.0",
    "slug": "whatsmenu-print-v2",
    "scheme": "whatsmenu-print-v2",
    "version": "1.0.8",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "@config-plugins/react-native-ble-plx",
        {
          "isBackgroundEnabled": true,
          "modes": [
            "peripheral",
            "central"
          ],
          "bluetoothAlwaysPemisson": "Allow $(PRODUCT_NAME) to connect to bluetooth devices"
        }
      ],
      "@notifee/react-native",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 33,
            "targetSdkVersion": 33,
            "buildToolsVersion": "33.0.0"
          }
        }
      ],
      ["./config_plugins/withModifyBluetoothPermission"]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFF"
      },
      "permissions": [
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.WAKE_LOCK",
        "android.permission.REQUEST_IGNORE_BATTERY_OPTIMIZATIONS",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.whatsmenu.whatsmenuprintv2",
      "versionCode": 10
    },
    "extra": {
      "eas": {
        "projectId": "1a7e419d-19c7-4ab8-9416-306d7470f60b"
      }
    },
    "ios": {
      "bundleIdentifier": "com.whatsmenu.whatsmenuprintv2"
    }
  }
}
