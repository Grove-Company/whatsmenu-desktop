{
  "extends": ["plugin:jsdoc/recommended", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": ["error", { "singleQuote": true, "semi": true }],
    "jsdoc/require-param-type": "error"
  },
  "plugins": ["jsdoc"]
}
