<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <title>Document</title>

  <style>
    .text-center {
      text-align: center;
    }
    .ttu {
      text-transform: uppercase;
    }
    .printer-ticket {
      display: table !important;
      /* width: {{profile.options.print.width}}; */
      width: 100%;
      /* max-width: 400px; */
      font-weight: 999;
      line-height: 1.3em;
      color: #000;
      background:transparent !important;
      color:#000 !important;
      text-shadow:none !important;
      filter:none !important;
      -ms-filter:none !important;
    }
    table.printer-ticket,
    table.printer-ticket * {
      font-family: "Times New Roman", Arial, Helvetica, sans-serif;
      font-size: 9.035143pt;
    }
    .printer-ticket th:nth-child(2),
    .printer-ticket td:nth-child(2) {
      width: 50px;
    }
    .printer-ticket th:nth-child(3),
    .printer-ticket td:nth-child(3) {
      width: 90px;
      text-align: right;
    }
    .printer-ticket th {
      font-weight: inherit;
      padding: 10px 0;
      text-align: center;
      border-bottom: 1px dashed #000;
    }
    .printer-ticket tbody tr:last-child td {
      padding-bottom: 10px;
    }
    .printer-ticket tfoot .sup td {
      padding: 10px 0;
      border-top: 1px dashed #000;
    }
    .printer-ticket tfoot .sup.p--0 td {
      padding-bottom: 0;
    }
    .printer-ticket .title {
      font-size: 18.070286pt;
      padding: 15px 0;
    }
    .printer-ticket .top td {
      padding-top: 10px;
    }
    .printer-ticket .last td {
      padding-bottom: 10px;
    }


    .printer-ticket tbody td.divider {
      padding-top: 10px;
      border-top: 2px dashed #000;
    }
  </style>
</head>
<body>

  <h1 style="text-align: center">{{profile.name}}</h1>
<table id="tablePrint" class="printer-ticket" style="width: 100%; padding-right: 5px;">
  {{--  <thead>  --}}
    {{--  </thead>  --}}
  <tbody class="ticketItens">
    <tr>
      {{--  <td class="title" colspan="3"><h1>{{ profile.name }}</h1></td>  --}}
    </tr>
    <tr>
      <td colspan="3" class="ticketDate"></td>
    </tr>
    <tr>
      <td colspan="3" class="ticketClient">

      </td>
    </tr>
    <tr>
      <td class="ttu" colspan="3">
        <b class="ticketCode"></b>
      </td>
    </tr>

   <tr class="top ttu">
     <td colspan="3"></td>
   </tr>

   <tr class="top ttu">
     <td colspan="3"></td>
   </tr>

   <tr>
     <td colspan="3" class="ticketFooter"></td>
   </tr>
  </tbody>
</table>


<script>
  localStorage.csrf = '{{ csrfToken }}';
  localStorage.profileName = '{{ profile.name }}';
  </script>

  <script src="/dist/jquery-3.6.0.min.js"></script>
  {{--  <script src="https://code.jquery.com/jquery-migrate-3.3.2.min.js"></script>  --}}
  {{ script('/dist/luxon.min.js') }}

