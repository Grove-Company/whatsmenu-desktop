<!doctype html>
<html lang="pt-BR">
<head>
  <meta charset="utf-8">
  <title>{{ profile.name }}</title>
  @if(type === "default" && !ogImage)
    <meta property="og:title" content="{{ profile.name }}">
    <meta property="og:description" content="{{ profile.description }}">
    <meta property="og:url" content="https://whatsmenu.com.br/{{ profile.slug }}/">
    <meta property="og:image" content="{{ profile.logo }}">
    <meta property="og:image:alt" content="{{ profile.logo }}">
  @elseif(ogImage)
    <meta property="og:title" content="{{profile.name}} - Venha conferir nossos itens">
    <meta property="og:description" content="{{ profile.description }}">
    <meta property="og:url" content="https://whatsmenu.com.br/{{ profile.slug }}/">
    <meta property="og:image" content="/img/socialmedia/{{ ogImage }}">
    <meta property="og:image:width" content="1080" />
    <meta property="og:image:height" content="1080" />
    <meta property="og:image:alt" content="/img/socialmedia/{{ ogImage }}">
  @elseif(type === "product")
    <meta property="og:title" content="{{profile.name}} - {{product.name}}">
    <meta property="og:description" content="{{ product.description ? product.description : profile.description}}">
    <meta property="og:url" content="https://whatsmenu.com.br/{{ profile.slug }}/{{category.id}}/{{product.id}}">
    <meta property="og:image" content="{{ product.image ? product.image : profile.logo}}">
    <meta property="og:image:alt" content="{{ product.image }}">
  @elseif(type === "order")
    <meta property="og:title" content="{{profile.name}} - Status do pedido">
    <meta property="og:description" content="{{ profile.description}}">
    <meta property="og:url" content="https://whatsmenu.com.br/{{ profile.slug }}/status/{{cart.code}}">
    <meta property="og:image" content="{{ profile.logo}}">
    <meta property="og:image:alt" content="{{ profile.logo }}">
  @else
    <meta property="og:title" content="{{ profile.name }} - {{category.name}}">
    <meta property="og:description" content="{{ profile.description }}">
    <meta property="og:url" content="https://whatsmenu.com.br/{{ profile.slug }}/{{category.id}}">
    <meta property="og:image" content="{{ profile.logo }}">
    <meta property="og:image:alt" content="{{ profile.logo }}">
  @endif

